{
    "compilerOptions": {
        "outDir": "dist",
        "lib": ["es6", "dom"],
        "target": "es6",
        "module": "commonjs",
        "alwaysStrict": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "emitDecoratorMetadata": true,
        "types": [
            "@types/node"
        ]
    },
	"include": [
		"**/*.ts"
    ],
    "exclude": [
        
    ]
}